<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#494FEF"
    android:orientation="vertical">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/textViewExample"

        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.appcompat.widget.Toolbar

            android:id="@+id/toolbar2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#494FEF"
            android:minHeight="80dp"
            android:theme="?attr/actionBarTheme"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_editor_absoluteX="0dp" />

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="134dp"
            android:layout_height="30dp"
            android:layout_marginTop="24dp"
            android:contentDescription="@string/BrandText"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.299"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/quicktech" />

        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_marginEnd="16dp"
            android:background="#00FFFFFF"
            android:contentDescription="@string/cart_icon"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/carrito" />

        <ImageButton
            android:id="@+id/imageButton4"
            android:layout_width="49dp"
            android:layout_height="49dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:background="#00FFFFFF"
            android:contentDescription="@string/menu_icon"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/baseline_menu_24" />

    </androidx.constraintlayout.widget.ConstraintLayout>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="871dp"
        android:background="@color/white"
        android:visibility="visible">


        <TextView
            android:id="@+id/bgsegundo"
            android:layout_width="match_parent"
            android:layout_height="61dp"
            android:background="#494FEF"
            android:fontFamily="sans-serif-black"
            android:textAlignment="center"
            android:textAllCaps="true"
            android:textColor="#FFFFFF"
            android:textSize="25sp"
            android:textStyle="bold"
            android:typeface="sans"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/button_disponibilidad"
            android:layout_width="103dp"
            android:layout_height="23dp"
            android:layout_marginTop="28dp"
            android:background="@drawable/botonrounded"
            android:fontFamily="sans-serif-condensed-medium"
            android:shadowColor="#FFFFFF"
            android:text="@string/disponibilidad"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="11sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.931"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/titulocategoria"
            android:layout_width="114dp"
            android:layout_height="23dp"
            android:layout_marginTop="16dp"
            android:fontFamily="sans-serif-black"
            android:text="Notebooks"
            android:textAlignment="center"
            android:textAllCaps="true"
            android:textColor="#FFFFFF"
            android:textSize="15sp"
            android:textStyle="normal"
            android:typeface="sans"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.074"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <ImageView
            android:id="@+id/fondito"
            android:layout_width="411dp"
            android:layout_height="585dp"
            android:background="#D9D9D9"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/bgsegundo" />

        <TextView
            android:id="@+id/tituloruta"
            android:layout_width="116dp"
            android:layout_height="19dp"
            android:layout_marginTop="20dp"
            android:fontFamily="sans-serif-condensed"
            android:text="Inicio > Notebooks >"
            android:textAlignment="center"
            android:textAllCaps="true"
            android:textColor="#000"
            android:textSize="11sp"
            android:textStyle="normal"
            android:typeface="sans"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.074"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/fondito" />


        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="155dp"
            android:layout_height="206dp"
            android:layout_marginTop="60dp"
            app:layout_constraintEnd_toStartOf="@+id/imageView5"
            app:layout_constraintHorizontal_bias="0.687"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/bgsegundo"
            app:srcCompat="@drawable/pr1" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="165dp"
            android:layout_height="207dp"
            app:srcCompat="@drawable/pr2"
            tools:layout_editor_absoluteX="211dp"
            tools:layout_editor_absoluteY="120dp" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="157dp"
            android:layout_height="199dp"
            app:srcCompat="@drawable/pr4"
            tools:layout_editor_absoluteX="212dp"
            tools:layout_editor_absoluteY="369dp" />

        <Button
            android:id="@+id/btnpaginas"
            android:layout_width="320dp"
            android:layout_height="24dp"
            android:background="@drawable/botonrounded"
            android:text="< 1  2  3  4  5  6  7  8 ...  12 >"
            android:fontFamily="sans-serif-black"
            android:textAlignment="center"
            android:textAllCaps="true"
            android:textColor="#FFFFFF"
            android:textSize="11sp"
            android:textStyle="bold"
            android:typeface="sans"
            tools:layout_editor_absoluteX="46dp"
            tools:layout_editor_absoluteY="614dp" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="154dp"
            android:layout_height="197dp"
            android:layout_marginStart="40dp"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/pr3"
            tools:layout_editor_absoluteY="369dp" />

        <Button
            android:id="@+id/btncarrito1"
            android:layout_width="155dp"
            android:layout_height="21dp"
            android:background="#494FEF"
            android:fontFamily="sans-serif-black"
            android:text="AÑADIR AL CARRITO"
            android:textAlignment="center"
            android:textAllCaps="true"
            android:textColor="#FFFFFF"
            android:textSize="11sp"
            android:textStyle="bold"
            android:typeface="sans"
            app:layout_constraintTop_toBottomOf="@+id/imageView3"
            tools:layout_editor_absoluteX="38dp" />

        <Button
            android:id="@+id/btncarrito2"
            android:layout_width="156dp"
            android:layout_height="21dp"
            android:background="#494FEF"
            android:fontFamily="sans-serif-black"
            android:text="AÑADIR AL CARRITO"
            android:textAlignment="center"
            android:textAllCaps="true"
            android:textColor="#FFFFFF"
            android:textSize="11sp"
            android:textStyle="bold"
            android:typeface="sans"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.36"
            app:layout_constraintStart_toEndOf="@+id/btncarrito1"
            app:layout_constraintTop_toBottomOf="@+id/imageView5" />

        <Button
            android:id="@+id/btncarrito3"
            android:layout_width="155dp"
            android:layout_height="19dp"
            android:layout_marginStart="40dp"
            android:background="#494FEF"
            android:fontFamily="sans-serif-black"
            android:text="AÑADIR AL CARRITO"
            android:textAlignment="center"
            android:textAllCaps="true"
            android:textColor="#FFFFFF"
            android:textSize="11sp"
            android:textStyle="bold"
            android:typeface="sans"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView6" />

        <Button
            android:id="@+id/btncarrito4"
            android:layout_width="148dp"
            android:layout_height="21dp"
            android:layout_marginEnd="51dp"
            android:background="#494FEF"
            android:fontFamily="sans-serif-black"
            android:text="AÑADIR AL CARRITO"
            android:textAlignment="center"
            android:textAllCaps="true"
            android:textColor="#FFFFFF"
            android:textSize="11sp"
            android:textStyle="bold"
            android:typeface="sans"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView7" />


    </androidx.constraintlayout.widget.ConstraintLayout>


</LinearLayout>